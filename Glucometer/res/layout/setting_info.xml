<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="match_parent"
	android:layout_height="match_parent">
	<LinearLayout android:layout_height="wrap_content"
		android:layout_width="match_parent" android:id="@+id/linearLayout1">
		<TextView android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="Battery: "
			android:layout_gravity="center_vertical"></TextView>
		<TextView android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="0%"
			android:layout_gravity="center_vertical" android:id="@+id/batteryText"></TextView>
		<SeekBar android:layout_height="wrap_content" android:id="@+id/batterySeekBar"
			android:layout_gravity="center_vertical" android:layout_width="match_parent"
			android:layout_marginLeft="5dp" android:max="100"></SeekBar>
	</LinearLayout>
	<LinearLayout android:layout_height="wrap_content"
		android:layout_width="match_parent" android:id="@+id/linearLayout2">
		<TextView android:text="Initialization Error in next Test: " android:id="@+id/textView1"
			android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
		<CheckBox android:text="" android:id="@+id/initializationErrorCheckbox"
			android:layout_width="wrap_content" android:layout_height="wrap_content"></CheckBox>
	</LinearLayout>
	<LinearLayout android:layout_height="wrap_content"
		android:layout_width="match_parent" android:id="@+id/linearLayout3">
		<TextView android:text="Valid Strip in next test: " android:id="@+id/textView2"
			android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
		<CheckBox android:text="" android:id="@+id/stripValidCheckbox"
			android:layout_width="wrap_content" android:layout_height="wrap_content"></CheckBox>
	</LinearLayout>
	<LinearLayout android:layout_height="wrap_content"
		android:layout_width="match_parent" android:id="@+id/linearLayout4">
		<TextView android:text="Sufficient Blood in next Test: " android:id="@+id/textView3"
			android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
		<CheckBox android:text="" android:id="@+id/bloodSufficientCheckbox"
			android:layout_width="wrap_content" android:layout_height="wrap_content"></CheckBox>
	</LinearLayout>
	<LinearLayout android:layout_height="wrap_content"
		android:layout_width="match_parent" android:id="@+id/linearLayout5">
		<TextView android:text="PC connected: " android:id="@+id/textView4"
			android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
		<CheckBox android:text="" android:id="@+id/pcReadyCheckbox"
			android:layout_width="wrap_content" android:layout_height="wrap_content"></CheckBox>
	</LinearLayout>
	<LinearLayout android:layout_height="wrap_content"
		android:layout_width="match_parent" android:id="@+id/linearLayout6">
		<TextView android:text="Software Running: " android:id="@+id/textView5"
			android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
		<CheckBox android:text="" android:id="@+id/softwareReadyCheckbox"
			android:layout_width="wrap_content" android:layout_height="wrap_content"></CheckBox>
	</LinearLayout>

</LinearLayout>
