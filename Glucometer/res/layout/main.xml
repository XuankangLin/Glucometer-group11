<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<RelativeLayout android:layout_height="match_parent"
		android:id="@+id/relativeLayout1" android:layout_width="match_parent">
		<ImageView android:id="@+id/meterImage" android:src="@drawable/glucometer_not_beeping"
			android:layout_width="wrap_content" android:layout_height="match_parent"
			android:layout_alignParentTop="true" android:minWidth="250dp"
			android:layout_marginLeft="5dp"></ImageView>
		<ImageView android:src="@drawable/valid_test_strip_not_inserted"
			android:layout_width="wrap_content" android:id="@+id/testStripImage"
			android:layout_height="wrap_content" android:layout_alignParentTop="true"
			android:layout_marginLeft="80dp"></ImageView>
		<LinearLayout android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/linearLayout1"
			android:orientation="vertical" android:layout_centerVertical="true"
			android:layout_alignParentRight="true">
			<ImageView android:src="@drawable/pen"
				android:layout_height="20dp" android:id="@+id/resetImage"
				android:layout_width="60dp" android:layout_marginBottom="50dp"
				android:layout_gravity="right"></ImageView>
			<ImageView android:src="@drawable/usb_not_inserted"
				android:id="@+id/usbImage" android:layout_marginTop="80dp"
				android:layout_width="70dp" android:layout_height="30dp"
				android:layout_gravity="right"></ImageView>
		</LinearLayout>
		<ImageView android:id="@+id/acPlugImage" android:src="@drawable/ac_plug"
			android:layout_width="wrap_content" android:layout_height="120dp"
			android:layout_alignParentBottom="true"
			android:layout_marginLeft="110dp"></ImageView>
		<ImageView android:id="@+id/buttonImage" android:src="@drawable/button_up"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:layout_marginLeft="100dp" android:layout_marginTop="350dp"></ImageView>
		<include android:layout_width="220dp" layout="@layout/screen"
			android:layout_height="120dp" android:id="@+id/include1"
			android:layout_marginTop="200dp" android:layout_marginLeft="20dp"></include>
	</RelativeLayout>
</LinearLayout>
